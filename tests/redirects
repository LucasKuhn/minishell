grep hi <./tests/test_files/infile
grep hi "<infile" <         ./tests/test_files/infile
echo hi < ./tests/test_files/infile bye bye
grep hi <./tests/test_files/infile_big <./tests/test_files/infile
echo <"./tests/test_files/infile" "bonjour       42"
cat <"./tests/test_files/file name with spaces"
cat <./tests/test_files/infile_big ./tests/test_files/infile
cat <"1""2""3""4""5"
echo <"./tests/test_files/infile" <missing <"./tests/test_files/infile"
echo <missing <"./tests/test_files/infile" <missing
cat <"./tests/test_files/infile"
echo <"./tests/test_files/infile_big" | cat <"./tests/test_files/infile"
echo <"./tests/test_files/infile_big" | cat "./tests/test_files/infile"
echo <"./tests/test_files/infile_big" | echo <"./tests/test_files/infile"
echo hi | cat <"./tests/test_files/infile"
echo hi | cat "./tests/test_files/infile"
cat <"./tests/test_files/infile" | echo hi
cat <"./tests/test_files/infile" | grep hello
cat <"./tests/test_files/infile_big" | echo hi
cat <missing
cat <missing | cat
cat <missing | echo oi
cat <missing | cat <"./tests/test_files/infile"
echo <123 <456 hi | echo 42
ls >./tests/test_files/outfile01
ls >         ./tests/test_files/outfile01
echo hi >         ./tests/test_files/outfile01 bye
ls >./tests/test_files/outfile01 >./tests/test_files/outfile02
ls >./tests/test_files/outfile01 >./tests/test_files/invalid_permission
ls >"./tests/test_files/outfile with spaces"
ls >"./tests/test_files/outfile""1""2""3""4""5"
